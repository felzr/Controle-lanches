<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_relatorio"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/fundo"
    tools:context="com.example.a709151034.controledelanches.Activitys.MenuActivity">

    <TextView
        android:text="Controle de Lanches"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView11"
        android:textAppearance="@android:style/TextAppearance.DeviceDefault.Widget.ActionBar.Title"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/lucro"
        android:layout_marginRight="15dp"
        android:layout_marginEnd="15dp"
        android:id="@+id/btVendas"
        android:layout_alignTop="@+id/btEstoque"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:onClick="Vendas" />

    <TextView
        android:text="    Vendas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="7dp"
        android:layout_marginEnd="7dp"
        android:id="@+id/textView2"
        android:textAppearance="@android:style/TextAppearance.Material.Large"
        android:layout_below="@+id/btVendas"
        android:layout_alignRight="@+id/btVendas"
        android:layout_alignEnd="@+id/btVendas"
        android:layout_alignLeft="@+id/btVendas"
        android:layout_alignStart="@+id/btVendas" />

    <TextView
        android:text="   Estoque"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView7"
        android:textAppearance="@android:style/TextAppearance.Material.Large"
        android:layout_below="@+id/btEstoque"
        android:layout_alignRight="@+id/btEstoque"
        android:layout_alignEnd="@+id/btEstoque"
        android:layout_alignLeft="@+id/btEstoque"
        android:layout_alignStart="@+id/btEstoque" />

    <TextView
        android:text="  Exportar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView8"
        android:textAppearance="@android:style/TextAppearance.Material.Large"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/btExportar"
        android:layout_alignStart="@+id/btExportar"
        android:layout_marginLeft="9dp"
        android:layout_marginStart="9dp"
        android:layout_marginBottom="93dp"
        android:layout_alignRight="@+id/btExportar"
        android:layout_alignEnd="@+id/btExportar" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/relatorios"
        android:id="@+id/btExportar"
        android:layout_above="@+id/textView8"
        android:layout_alignLeft="@+id/textView7"
        android:layout_alignStart="@+id/textView7" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/pedidosrelatorio"
        android:id="@+id/btPedido"
        android:layout_alignTop="@+id/btExportar"
        android:layout_alignRight="@+id/btVendas"
        android:layout_alignEnd="@+id/btVendas"
        android:onClick="Produto" />

    <TextView
        android:text="   Produtos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView25"
        android:textAppearance="@android:style/TextAppearance.Material.Large"
        android:layout_below="@+id/btPedido"
        android:layout_alignRight="@+id/btPedido"
        android:layout_alignEnd="@+id/btPedido"
        android:layout_alignLeft="@+id/btPedido"
        android:layout_alignStart="@+id/btPedido" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/cestarelatorio"
        android:layout_marginLeft="18dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="74dp"
        android:onClick="Estoque"
        android:id="@+id/btEstoque"
        android:layout_below="@+id/textView11"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_menu_revert"
        android:layout_marginTop="10dp"
        android:id="@+id/imageButton"
        android:tint="@android:color/background_dark"
        android:layout_below="@+id/textView25"
        android:layout_toEndOf="@+id/textView11" />

</RelativeLayout>
